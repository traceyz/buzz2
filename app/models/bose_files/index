<script>

	jQuery('.product_info:first > :first-child').after('<div id="prod_rollup">&#160;</div>');

	var siteLocale = jQuery('html').attr('lang');

	var reviewsProductUrl = window.location.href;



		var ls135_iii_widgets = {
		    prod_rollup:function(){
		        var opts = {
		        elem:"prod_rollup"
		        }
		        pluckAppProxy.embedApp("pluck_reviews_rollup", {
		            plckReviewOnKeyType: "article",
					plckReviewOnKey: "ls135_iii",
		            plckReviewShowCreate :"true",
		            plckReviewShowAttributes:"true",
		            pluckLang: ""+siteLocale+""
		        }, opts); 
		    },
		    reviews_rollup:function(){
		        var opts = {
				elem:"reviews_rollup"
		        }
		        pluckAppProxy.embedApp("pluck_reviews_rollup", {
		            plckReviewOnKeyType: "article",
		            plckReviewOnKey: "ls135_iii",
		            pluckLang: ""+siteLocale+""             
		        }, opts);
		    },
		    reviews_list:function(){
		        var opts = {
				elem:"reviews_list"
		        }
		        pluckAppProxy.embedApp("pluck_reviews_list", {
			        plckReviewOnKeyType: "article",
			        plckReviewOnKey: "ls135_iii",
			        plckReviewAuthorAttributeSetKey: "Bose_generic_characteristics",
			        plckArticleTitle: "Lifestyle 135 Series III home entertainment system",
			        plckArticleUrl: ""+reviewsProductUrl+"",
			        plckDiscoveryCategories: "Lifestyle 135 Series III home entertainment system, Lifestyle home theater system, Bose home theater, Bose soundbar systems, soundbar home theater systems, Wi-Fi home audio, bose, ",
			        plckDiscoverySection: "home theater",
			        pluckLang: ""+siteLocale+""                
		        }, opts);
		        
		        if(eval(pluckAppProxy.activityCallbacks)) {
			        for (var key in pluckAppProxy.activityCallbacks) {
			            pluckAppProxy.activityCallbacks[key].length = 0;
			            //console.log('Zeroed: pluckAppProxy.activityCallbacks.' + key + '.length = ' +  pluckAppProxy.activityCallbacks[key].length);
			        }
				} //else {
					//console.log('not initialized');
				//}

		        pluckAppProxy.registerActivityCallback("ReviewListRendered", plcOvrds.init);
		        pluckAppProxy.registerActivityCallback("ReviewCreate", plcOvrds.review_create);
		        pluckAppProxy.registerActivityCallback("ReviewSubmitRendered", plcOvrds.review_submit);


		    },
		    init:function(){
			    ls135_iii_widgets.prod_rollup();
			    ls135_iii_widgets.reviews_rollup();
			    ls135_iii_widgets.reviews_list();
		    }
		}

	
			ls135_iii_widgets.init();
		
</script>


                        
                                </section>
                            
                    
            
		</div>		
	</div>
</section>



        
